<div class="data-results">
  <div data-result="admin-edit-form-" class="admin-edit-form">
    <div class="admin-form-block-outer">

<%= form_for(object_instance, url: admin_form_url, class: "inline-form admin-edit-form", remote: true, data: {result_target: "#admin-item-#{object_instance.id}", preprocessor: "admin_result"}) do |f| %>
  <%= render partial: 'admin_handler/form_errors' %>
  <div class="form-group">
    <%= f.label :name %>
    <%= f.text_field :name %>
  </div>
  <div class="form-group">
    <%= f.label :value, 'data-add-icon' => 'info-sign', title: "#{object_instance.id ? 'Value for existing records can not be changed.' : 'Set internal value'}" %>
    <%= f.number_field :value, disabled: !!object_instance.id %>
  </div>
  <div class="form-group">
    <%= f.check_box :disabled %>
    <%= f.label :disable %>

  </div>
  <div class="form-group">
    <%= hidden_filter_fields %>
    <%= admin_submit_and_cancel(f) %>
  </div>

  <% end %>
    </div>
  </div>
</div>
