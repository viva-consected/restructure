<div class="data-results">
  <div data-result="admin-edit-form-"  class="admin-edit-form">
<%= form_for(object_instance,  class: "inline-form", remote: true) do |f| %>
  <%= render partial: 'admin_handler/form_errors' %>
  <div class="form-group">
    <%= f.label :name %>
    <%= f.text_field :name %>
      <p class="help-block">&nbsp;</p>
  </div>
  <div class="form-group">
    <%= f.label :value %>
    <%= f.text_field :value, style: 'width:300px;' %>
    <p class="help-block">Enter field names to be substituted like this: {{field_name}}</p>
  </div>
  <div class="form-group">
    <%= f.label :disable %>
    <%= f.check_box :disabled %>
  </div>
  <div class="form-group">
    <%= hidden_filter_fields %>
    <%= admin_submit_and_cancel(f) %>
  </div>
<% end %>
  </div>
</div>
